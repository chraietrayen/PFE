generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Account {
  id                String    @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  userId            String    @db.VarChar(36)
  type              String    @db.VarChar(255)
  provider          String    @db.VarChar(255)
  providerAccountId String    @db.VarChar(255)
  refresh_token     String?   @db.Text
  access_token      String?   @db.Text
  expires_at        Int?
  token_type        String?   @db.VarChar(255)
  scope             String?   @db.VarChar(255)
  id_token          String?   @db.Text
  session_state     String?   @db.VarChar(255)
  createdAt         DateTime? @default(now()) @db.DateTime(0)
  updatedAt         DateTime? @default(now()) @db.DateTime(0)

  @@unique([provider, providerAccountId], map: "provider_providerAccountId")
}

model Attendance {
  id         String             @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  employeeId String             @db.VarChar(36)
  date       DateTime           @db.Date
  checkIn    DateTime?          @db.DateTime(0)
  checkOut   DateTime?          @db.DateTime(0)
  status     Attendance_status? @default(PRESENT)
  notes      String?            @db.Text
  createdAt  DateTime?          @default(now()) @db.DateTime(0)
  updatedAt  DateTime?          @default(now()) @db.DateTime(0)
}

model ChatMessage {
  id        String                 @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  userId    String                 @db.VarChar(36)
  message   String                 @db.Text
  response  String?                @db.Text
  sentiment ChatMessage_sentiment?
  createdAt DateTime?              @default(now()) @db.DateTime(0)
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model Employe {
  id                 String                @id @db.VarChar(36)
  user_id            String                @unique(map: "user_id") @db.VarChar(36)
  nom                String?               @db.VarChar(255)
  prenom             String?               @db.VarChar(255)
  email              String?               @db.VarChar(255)
  birthday           DateTime?             @db.Date
  sexe               Employe_sexe?
  rib                String?               @db.VarChar(24)
  adresse            String?               @db.VarChar(500)
  telephone          String?               @db.VarChar(50)
  date_embauche      DateTime?             @db.Date
  photo              String?               @db.MediumText
  cv                 String?               @db.MediumText
  autres_documents   String?               @db.LongText
  type_contrat       Employe_type_contrat?
  created_at         DateTime?             @default(now()) @db.DateTime(0)
  updated_at         DateTime?             @default(now()) @db.DateTime(0)
  rejection_reason   String?               @db.Text
  approved_by        String?
  approved_at        DateTime?
  status             Employe_status?       @default(EN_ATTENTE)
  contract_file      String?               @db.MediumText
  contract_signature String?               @db.MediumText
  contract_signed_at DateTime?             @db.DateTime(0)
  base_salary        Float?                @db.Double
  hourly_rate        Float?                @db.Double
  annual_leave       Int?                  @default(26)
  User               User                  @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fk_employe_user")
  rh_decisions       rh_decisions[]

  @@index([user_id], map: "idx_user_id")
}

model LeaveRequest {
  id         String               @id
  user_id    String
  type       LeaveRequest_type
  start_date DateTime             @db.Date
  end_date   DateTime             @db.Date
  duration   Int
  reason     String               @db.Text
  status     LeaveRequest_status? @default(PENDING)
  decided_by String?
  decided_at DateTime?            @db.DateTime(0)
  comments   String?              @db.Text
  created_at DateTime?            @default(now()) @db.DateTime(0)
  updated_at DateTime?            @default(now()) @db.DateTime(0)

  @@index([start_date, end_date], map: "idx_dates")
  @@index([status], map: "idx_status")
  @@index([user_id, status], map: "idx_user_status")
}

model Permission {
  id             String            @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  module         String            @db.VarChar(50)
  type           Permission_type
  action         Permission_action
  createdAt      DateTime?         @default(now()) @db.DateTime(0)
  updatedAt      DateTime?         @default(now()) @db.DateTime(0)
  RolePermission RolePermission[]
}

model Pointage {
  id                 String           @id
  user_id            String
  type               Pointage_type
  timestamp          DateTime         @default(now()) @db.DateTime(0)
  status             Pointage_status? @default(VALID)
  check_in_id        String?
  device_fingerprint String?          @db.Text
  ip_address         String?          @db.VarChar(100)
  geolocation        Json?
  captured_photo     String?          @db.LongText
  face_verified      Boolean?         @default(false)
  verification_score Int?
  hours_worked       Decimal?         @db.Decimal(5, 2)
  anomaly_detected   Boolean?         @default(false)
  anomaly_reason     String?          @db.Text
  created_at         DateTime?        @default(now()) @db.DateTime(0)
  updated_at         DateTime?        @default(now()) @db.DateTime(0)

  @@index([check_in_id], map: "idx_check_in")
  @@index([status], map: "idx_status")
  @@index([type], map: "idx_type")
  @@index([user_id, timestamp], map: "idx_user_timestamp")
}

model Role {
  id             String           @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  name           String           @unique(map: "name") @db.VarChar(50)
  description    String?          @db.Text
  createdAt      DateTime?        @default(now()) @db.DateTime(0)
  updatedAt      DateTime?        @default(now()) @db.DateTime(0)
  RolePermission RolePermission[]
}

model RolePermission {
  id           String     @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  roleId       String     @db.VarChar(36)
  permissionId String     @db.VarChar(36)
  createdAt    DateTime?  @default(now()) @db.DateTime(0)
  Role         Role       @relation(fields: [roleId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "RolePermission_ibfk_1")
  Permission   Permission @relation(fields: [permissionId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "RolePermission_ibfk_2")

  @@unique([roleId, permissionId], map: "role_permission_unique")
  @@index([permissionId], map: "permissionId")
}

model Session {
  id           String    @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  sessionToken String    @unique(map: "sessionToken") @db.VarChar(255)
  userId       String    @db.VarChar(36)
  expires      DateTime  @db.DateTime(0)
  createdAt    DateTime? @default(now()) @db.DateTime(0)
  updatedAt    DateTime? @default(now()) @db.DateTime(0)
}

model User {
  id                       String                @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  email                    String                @unique(map: "email") @db.VarChar(255)
  name                     String?               @db.VarChar(255)
  password                 String?               @db.VarChar(255)
  role                     User_role?            @default(USER)
  image                    String?               @db.VarChar(500)
  emailVerified            DateTime?             @db.DateTime(0)
  createdAt                DateTime?             @default(now()) @db.DateTime(0)
  updatedAt                DateTime?             @default(now()) @db.DateTime(0)
  status                   User_status           @default(INACTIVE)
  Employe                  Employe?
  UserPreferences          UserPreferences?
  audit_logs               audit_logs[]
  cookie_consents          cookie_consents[]
  demande_conge            demande_conge[]
  device_fingerprints      device_fingerprints[]
  dossier_employe          dossier_employe[]
  notifications            notifications[]
  rh_decisions             rh_decisions[]
  attendance_sessions      attendance_sessions[]
  reward_days_user         reward_days[]         @relation("reward_days_user")
  reward_days_granted_by   reward_days[]         @relation("reward_days_granted_by")
  salary_reports_user      salary_reports[]      @relation("salary_reports_user")
  salary_reports_reviewer  salary_reports[]      @relation("salary_reports_reviewer")
  salary_reports_approver  salary_reports[]      @relation("salary_reports_approver")
}

model UserPreferences {
  id                 String    @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  userId             String    @unique(map: "userId") @db.VarChar(36)
  language           String?   @default("fr") @db.VarChar(10)
  emailNotifications Boolean?  @default(true)
  pushNotifications  Boolean?  @default(true)
  soundAlerts        Boolean?  @default(true)
  theme              String?   @default("system") @db.VarChar(20)
  timezone           String?   @default("Europe/Paris") @db.VarChar(100)
  createdAt          DateTime? @default(now()) @db.DateTime(0)
  updatedAt          DateTime? @default(now()) @db.DateTime(0)
  User               User      @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "UserPreferences_ibfk_1")
}

model VerificationToken {
  identifier String   @db.VarChar(255)
  token      String   @unique(map: "token") @db.VarChar(255)
  expires    DateTime @db.DateTime(0)

  @@id([identifier, token])
}

model anomalies {
  id          String             @id
  type        anomalies_type
  severity    anomalies_severity
  entity_type String
  entity_id   String
  pointage_id String?
  description String             @db.Text
  metadata    String?            @db.LongText
  status      anomalies_status   @default(PENDING)
  resolved_by String?            @db.VarChar(36)
  resolved_at DateTime?
  resolution  String?            @db.Text
  created_at  DateTime           @default(now())

  @@index([created_at])
  @@index([pointage_id], map: "anomalies_pointage_id_fkey")
  @@index([severity, status])
  @@index([type, status])
}

model audit_logs {
  id          String              @id
  user_id     String?             @db.VarChar(36)
  action      String
  entity_type String
  entity_id   String?
  changes     String?             @db.LongText
  ip_address  String?
  user_agent  String?             @db.Text
  metadata    String?             @db.LongText
  severity    audit_logs_severity @default(INFO)
  created_at  DateTime            @default(now())
  User        User?               @relation(fields: [user_id], references: [id])

  @@index([user_id, created_at])
}

model contracts {
  id                String           @id @db.VarChar(36)
  title             String           @db.VarChar(255)
  description       String?          @db.Text
  type              contracts_type
  status            contracts_status @default(DRAFT)
  original_pdf_path String           @db.Text
  signed_pdf_path   String?          @db.Text
  signature_data    String?          @db.LongText
  user_id           String
  created_by        String?          @db.VarChar(36)
  valid_from        DateTime?
  sent_at           DateTime?        @db.DateTime(0)
  viewed_at         DateTime?
  archived_at       DateTime?
  signed_at         DateTime?        @db.DateTime(0)
  signed_ip_address String?
  signed_user_agent String?          @db.Text
  valid_until       DateTime?
  version           Int?             @default(1)
  signature_zone    String?          @db.Text
  parent_id         String?
  metadata          Json?
  created_at        DateTime?        @default(now()) @db.DateTime(0)
  updated_at        DateTime?        @default(now()) @db.DateTime(0)

  @@index([created_by], map: "idx_contracts_created_by")
  @@index([user_id], map: "idx_contracts_employee")
  @@index([status], map: "idx_contracts_status")
}

model cookie_consents {
  id              String    @id @db.VarChar(36)
  session_id      String?   @db.VarChar(36)
  user_id         String?   @db.VarChar(36)
  necessary       Boolean?  @default(true)
  functional      Boolean?  @default(false)
  analytics       Boolean?  @default(false)
  marketing       Boolean?  @default(false)
  consent_version String?   @default("1.0") @db.VarChar(10)
  consent_method  String?   @default("banner") @db.VarChar(50)
  ip_address      String?   @db.VarChar(45)
  user_agent      String?   @db.Text
  created_at      DateTime? @default(now()) @db.DateTime(0)
  updated_at      DateTime? @default(now()) @db.DateTime(0)
  User            User?     @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "cookie_consents_ibfk_1")

  @@index([created_at], map: "idx_created_at")
  @@index([session_id], map: "idx_session_id")
  @@index([user_id], map: "idx_user_id")
}

model demande_conge {
  id               String                         @id
  userId           String                         @db.VarChar(36)
  type             demande_conge_type?
  date_debut       DateTime
  date_fin         DateTime
  status           demande_conge_status           @default(EN_ATTENTE)
  commentaire      String?
  is_half_day      Boolean?                       @default(false)
  half_day_session demande_conge_half_day_session?
  impact_on_salary Boolean?                       @default(true)
  approved_by_id   String?                        @db.VarChar(36)
  approved_at      DateTime?                      @db.DateTime(3)
  duration_days    Float?                         @db.Double
  User             User                           @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId], map: "demande_conge_userId_fkey")
}

model device_fingerprints {
  id                String   @id
  user_id           String   @db.VarChar(36)
  fingerprint       String   @db.Text
  user_agent        String   @db.Text
  platform          String?
  browser           String?
  screen_resolution String?
  timezone          String?
  language          String?
  first_seen        DateTime @default(now())
  last_seen         DateTime @default(now())
  is_trusted        Boolean  @default(false)
  User              User     @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@index([user_id])
}

model attendance_sessions {
  id                  String                        @id @default(dbgenerated("(uuid())")) @db.VarChar(191)
  user_id             String                        @db.VarChar(36)
  date                DateTime                      @db.Date
  session_type        attendance_sessions_session_type
  check_in_time       DateTime?                     @db.DateTime(3)
  check_out_time      DateTime?                     @db.DateTime(3)
  duration_minutes    Int?
  status              attendance_sessions_status    @default(PRESENT)
  check_in_photo      String?                       @db.LongText
  check_out_photo     String?                       @db.LongText
  check_in_ip         String?                       @db.VarChar(45)
  check_out_ip        String?                       @db.VarChar(45)
  device_fingerprint  String?                       @db.Text
  anomaly_detected    Boolean?                      @default(false)
  anomaly_reason      String?                       @db.Text
  created_at          DateTime                      @default(now()) @db.DateTime(3)
  updated_at          DateTime                      @default(now()) @db.DateTime(3)
  User                User                          @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@index([user_id])
  @@index([date])
  @@unique([user_id, date, session_type])
}

model reward_days {
  id              String             @id @default(dbgenerated("(uuid())")) @db.VarChar(191)
  user_id         String             @db.VarChar(36)
  granted_by_id   String             @db.VarChar(36)
  date            DateTime           @db.Date
  reason          String             @db.Text
  status          reward_days_status @default(APPROVED)
  salary_impact   Float?             @db.Double
  created_at      DateTime           @default(now()) @db.DateTime(3)
  updated_at      DateTime           @default(now()) @db.DateTime(3)
  User            User               @relation("reward_days_user", fields: [user_id], references: [id], onDelete: Cascade)
  GrantedBy       User               @relation("reward_days_granted_by", fields: [granted_by_id], references: [id], onDelete: Cascade)

  @@index([user_id])
  @@index([date])
}

model salary_reports {
  id                       String                 @id @default(dbgenerated("(uuid())")) @db.VarChar(191)
  user_id                  String                 @db.VarChar(36)
  month                    Int
  year                     Int
  contract_type            String?                @db.VarChar(50)
  base_salary              Float?                 @db.Double
  hourly_rate              Float?                 @db.Double
  worked_days              Float?                 @db.Double
  expected_work_days       Float?                 @db.Double
  worked_hours             Float?                 @db.Double
  expected_work_hours      Float?                 @db.Double
  absence_days             Float?                 @db.Double
  absence_hours            Float?                 @db.Double
  unpaid_absence_days      Float?                 @db.Double
  paid_leave_days          Float?                 @db.Double
  unpaid_leave_days        Float?                 @db.Double
  sick_leave_days          Float?                 @db.Double
  other_leave_days         Float?                 @db.Double
  reward_days              Float?                 @db.Double
  reward_salary            Float?                 @db.Double
  overtime_hours           Float?                 @db.Double
  overtime_rate            Float?                 @db.Double
  overtime_salary          Float?                 @db.Double
  gross_salary             Float?                 @db.Double
  deductions               Float?                 @db.Double
  net_salary               Float?                 @db.Double
  salary_breakdown         String?                @db.LongText
  status                   salary_reports_status  @default(DRAFT)
  reviewed_by_id           String?                @db.VarChar(36)
  reviewed_at              DateTime?              @db.DateTime(3)
  approved_by_id           String?                @db.VarChar(36)
  approved_at              DateTime?              @db.DateTime(3)
  created_at               DateTime               @default(now()) @db.DateTime(3)
  updated_at               DateTime               @default(now()) @db.DateTime(3)
  User                     User                   @relation("salary_reports_user", fields: [user_id], references: [id], onDelete: Cascade)
  Reviewer                 User?                  @relation("salary_reports_reviewer", fields: [reviewed_by_id], references: [id], onDelete: SetNull)
  Approver                 User?                  @relation("salary_reports_approver", fields: [approved_by_id], references: [id], onDelete: SetNull)

  @@index([user_id])
  @@index([month, year])
  @@unique([user_id, month, year])
}

model documents {
  id              String                   @id @db.VarChar(36)
  title           String                   @db.VarChar(255)
  description     String?                  @db.Text
  document_type   documents_document_type? @default(OTHER)
  category        documents_category?      @default(PERSONAL)
  status          documents_status?        @default(DRAFT)
  file_url        String?                  @db.VarChar(500)
  file_size       Int?
  file_type       String?                  @db.VarChar(100)
  owner_id        String                   @db.VarChar(36)
  uploaded_by     String?                  @db.VarChar(36)
  metadata        Json?
  is_confidential Boolean?                 @default(false)
  expires_at      DateTime?                @db.DateTime(0)
  created_at      DateTime?                @default(now()) @db.DateTime(0)
  updated_at      DateTime?                @default(now()) @db.DateTime(0)

  @@index([owner_id], map: "idx_documents_owner")
  @@index([status], map: "idx_documents_status")
  @@index([document_type], map: "idx_documents_type")
}

model dossier_employe {
  id             String               @id
  userId         String               @db.VarChar(36)
  type           dossier_employe_type
  titre          String
  description    String?
  fichier        String?
  date_obtention DateTime?
  User           User                 @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId], map: "dossier_employe_userId_fkey")
}

model login_history {
  id                 String    @id @db.VarChar(36)
  user_id            String    @db.VarChar(36)
  ip_address         String?   @db.VarChar(45)
  user_agent         String?   @db.Text
  login_method       String?   @default("credentials") @db.VarChar(50)
  success            Boolean?  @default(true)
  failure_reason     String?   @db.VarChar(255)
  device_fingerprint String?   @db.VarChar(255)
  is_suspicious      Boolean?  @default(false)
  created_at         DateTime? @default(now()) @db.DateTime(0)
  updated_at         DateTime? @default(now()) @db.DateTime(0)

  @@index([created_at], map: "idx_login_history_created")
  @@index([is_suspicious], map: "idx_login_history_suspicious")
  @@index([user_id], map: "idx_login_history_user")
}

model notifications {
  id         String                 @id
  user_id    String                 @db.VarChar(36)
  type       notifications_type
  title      String
  message    String                 @db.Text
  metadata   String?                @db.LongText
  is_read    Boolean                @default(false)
  read_at    DateTime?
  priority   notifications_priority @default(NORMAL)
  created_at DateTime               @default(now())
  User       User                   @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@index([user_id, is_read])
}

model otp_tokens {
  id         String    @id @db.VarChar(36)
  email      String    @db.VarChar(255)
  code       String    @db.VarChar(255)
  expires    DateTime  @db.DateTime(0)
  verified   Boolean?  @default(false)
  attempts   Int?      @default(0)
  created_at DateTime? @default(now()) @db.DateTime(0)

  @@index([email], map: "idx_email")
  @@index([expires], map: "idx_expires")
}

model password_reset_tokens {
  id         String   @id
  email      String
  token      String   @unique
  expires    DateTime
  created_at DateTime @default(now())

  @@unique([email, token])
}

model rh_decisions {
  id         String                @id
  employe_id String                @db.VarChar(36)
  decider_id String                @db.VarChar(36)
  decision   rh_decisions_decision
  reason     String?               @db.Text
  comments   String?               @db.Text
  metadata   String?               @db.LongText
  created_at DateTime              @default(now())
  User       User                  @relation(fields: [decider_id], references: [id], onDelete: Cascade)
  Employe    Employe               @relation(fields: [employe_id], references: [id], onDelete: Cascade)

  @@index([decider_id])
  @@index([employe_id])
}

model rh_favorites {
  id            String                     @id @db.VarChar(36)
  rh_user_id    String                     @db.VarChar(36)
  favorite_type rh_favorites_favorite_type
  target_id     String                     @db.VarChar(36)
  notes         String?                    @db.Text
  created_at    DateTime?                  @default(now()) @db.DateTime(0)
  updated_at    DateTime?                  @default(now()) @db.DateTime(0)

  @@unique([rh_user_id, favorite_type, target_id], map: "unique_favorite")
  @@index([target_id], map: "idx_rh_favorites_target")
  @@index([favorite_type], map: "idx_rh_favorites_type")
  @@index([rh_user_id], map: "idx_rh_favorites_user")
}

enum anomalies_type {
  UNUSUAL_HOURS
  MULTIPLE_DEVICES
  FACE_VERIFICATION_FAIL
  IMPOSSIBLE_LOCATION
  DUPLICATE_POINTAGE
  SUSPICIOUS_PATTERN
  MISSING_CHECKOUT
  SYSTEM_ERROR
}

enum LeaveRequest_type {
  PAID_LEAVE
  SICK_LEAVE
  UNPAID_LEAVE
  MATERNITY_LEAVE
  PATERNITY_LEAVE
}

enum Permission_type {
  BASE
  ADVANCED
}

enum Pointage_type {
  IN
  OUT
}

enum anomalies_severity {
  LOW
  MEDIUM
  HIGH
  CRITICAL
}

enum demande_conge_type {
  PAID
  UNPAID
  MATERNITE
  MALADIE
  PREAVIS
}

enum demande_conge_half_day_session {
  MORNING
  AFTERNOON
}

enum dossier_employe_type {
  DIPLOME
  CERTIFICAT
  ATTESTATION
}

enum notifications_type {
  PROFILE_APPROVED
  PROFILE_REJECTED
  POINTAGE_ANOMALY
  LEAVE_REQUEST
  SYSTEM_ALERT
  RH_ACTION_REQUIRED
  DOCUMENT_REQUIRED
  GENERAL
}

enum rh_favorites_favorite_type {
  USER
  DOCUMENT
  CONTRACT
}

enum Permission_action {
  ADD
  DELETE
  EDIT
  VIEW
  ACTIVATE
  VALIDATE
  VERIFY
  PROCESS
}

enum contracts_type {
  CDI
  CDD
  Stage
  Freelance
}

enum documents_document_type {
  CONTRACT
  ID_CARD
  CERTIFICATE
  PAYSLIP
  ATTESTATION
  OTHER
}

enum rh_decisions_decision {
  APPROVED
  REJECTED
  SUSPENDED
  REACTIVATED
  UPDATED
}

enum ChatMessage_sentiment {
  POSITIVE
  NEGATIVE
  NEUTRAL
}

enum Pointage_status {
  VALID
  ANOMALY
